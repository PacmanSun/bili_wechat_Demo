page{
  color: #666;
  padding: 10rpx;
}

/* navi_tabbar */
.navi{
  white-space: nowrap;
  padding: 5rpx 0;
}

.navi_item{
  padding: 20rpx 45rpx;
  font-size: 28rpx;
  display: inline-block;
}

.navi_item.active{
  border-bottom: 5rpx solid #de688b;
}
/* swiper slider */
.slider{
  margin-top: 10rpx;
}

.slider swiper{
  height: 220rpx;
}

.slider navigator{
  width: 100%;
  height: 100%;
}

.slider image{
  width: 100%;
  height: 100%;
}

/* videolist */
.video_list{
  display: flex;
  flex-wrap: wrap;
  padding: 5rpx;
  justify-content: space-between;
}

.video_list .video_list_item{
  width: 48%;
  margin-bottom: 20rpx;
}

.video_list_item .video_img_block{
  position: relative;
}

.video_img_block .video_img{
  width: 100%;
  border-radius: 16rpx;
}

.video_img_block .videoInfo_block{
  display: flex;
  justify-content: space-around;
  position: absolute;
  left: 0;
  bottom: 12rpx;
  width: 100%;
  color: #fff;
  font-size: 32rpx;
}

.video_list .videoTitle{
 display: -webkit-box;
 overflow: hidden;
 white-space: normal;
 text-overflow: ellipsis;
 word-wrap: break-word;
 -webkit-line-clamp: 2;
 -webkit-box-orient: vertical;
 font-size: 28rpx;
}